{
    "name": "laurent-hadjadj/docx-merge",
    "version": "1.1.0",
    "type": "library",
    "description": "Simple library for merging multiple Microsoft Word (.docx) files into one.",
    "keywords": [
        "docx",
        "merge",
        "placeholder"
    ],
    "license": "MIT",
    "homepage": "https://github.com/laurent-hadjadj/docx-merge",
    "authors": [
        {
            "name": "Laurent HADJADJ",
            "email": "laurent_h@me.com"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MaMoulinette\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaMoulinette\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
